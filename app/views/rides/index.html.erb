<%= link_to "Rides", '/rides' %>
<%= link_to "Amusement Parks", '/amusement_parks' %>

<h1>All Rides</h1>

<% @rides.each do |ride|%>
  <h3><%= ride.name %></h3>
  <h4>Is it a roller coaster? <%= ride.roller_coaster %></h4>
  <h4>Top Speed: <%= ride.top_speed %>MPH</h4>
  <h5> <%= link_to "Edit #{ride.name}", "/rides/#{ride.id}/edit" %> </h5>
<% end %>
