<%= link_to "Rides", '/rides' %>
<%= link_to "Amusement Parks", '/amusement_parks' %>
<%= link_to "New Amusement Park", '/amusement_parks/new' %>
<h1>All Amusement Parks</h1>

<% @amusement_parks.each do |amusement_park|%>
  <h3><%= amusement_park.name %></h3>
  <h5>Created on: <%= amusement_park.created_at %></h5>
  <p><%= link_to "Edit #{amusement_park.name}", "/amusement_parks/#{amusement_park.id}/edit" %></p>
  <p> <%= link_to "Delete #{amusement_park.name}", "/amusement_parks/#{amusement_park.id}", method: :delete %> </p>

<% end %>
